    "color-alpha": "^1.1.3",
    "dayjs": "^1.11.7",
    "install": "^0.13.0",
    "jwt-decode": "^3.1.2",
    "npm": "^9.8.1",
    "numbro": "^2.3.6",
    "patch-package": "^7.0.0",
    "pretty-format": "^29.5.0",
    "ramda": "^0.29.0",
    "react": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-if": "^4.1.4",
