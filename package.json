{
  "name": "EmumbaToastmasters",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=iPhone 13",
    "pod": "cd ios && pod install && cd ..",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "release": "cd android && ./gradlew assembleRelease && cd ..",
    "rm-bundle": "rm android/app/src/main/assets/index.android.bundle",
    "add-bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "gradle-clean": "cd android && ./gradlew clean",
    "link-fonts": "npx react-native-asset",
    "postinstall": "patch-package",
    "codepush-staging": "appcenter codepush release-react -a huzaifa.arshad-emumba.com/EmumbaToastmasters -d Staging",
    "codepush-production": "appcenter codepush release-react -a huzaifa.arshad-emumba.com/EmumbaToastmasters -d Production",
    "pretty": "prettier --write \"./**/*.{ts,tsx,js,jsx,json}\""
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.6",
    "@hookform/resolvers": "^3.1.0",
    "@react-native-async-storage/async-storage": "^1.18.1",
